{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "ES2020",
      "DOM"
    ],
    "module": "ESNext",
    "strict": true,
    "jsx": "preserve",
    "moduleResolution": "bundler",
    "types": [
      "vue"
    ]
  },
  "include": [
    "**/*.vue",
    "**/*.ts"
  ],
  "exclude": [
    "src/main.ts",
    "src/App.vue",
    "node_modules",
    "dist",
  ]
}